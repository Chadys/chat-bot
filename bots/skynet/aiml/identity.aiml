<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<category><pattern>QUEL EST TON NOM</pattern>
	<template>
        Je m'appelle <bot name="name"/>.<condition name="username"> :)
            <li value="*"></li>
            <li> Et toi ?
            <think><set name="topic">GETNAME</set></think></li>
        </condition>
    </template>
</category>

<category><pattern>QUEL EST VOTRE NOM</pattern>
	<template><srai>QUEL EST TON NOM<star/></srai></template>
</category>

<category><pattern>COMMENT T'APPELLES-TU</pattern>
	<template><srai>QUEL EST TON NOM<star/></srai></template>
</category>

